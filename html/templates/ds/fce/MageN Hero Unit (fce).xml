		</tx_templavoila>
		<type>array</type>
		<el type="array">
			<field_content type="array">
				<tx_templavoila type="array">
					<title>Content</title>
					<sample_data type="array">
						<numIndex index="0"></numIndex>
					</sample_data>
					<eType>ce</eType>
					<TypoScript>
10= RECORDS
10.source.current=1
10.tables = tt_content</TypoScript>
					<preview></preview>
